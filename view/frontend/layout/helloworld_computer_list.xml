<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account" />
    <body>
        <referenceBlock name="page.main.title">
            <action method="setPageTitle">
                <argument translate="true" name="title" xsi:type="string">My Computer List</argument>
            </action>
        </referenceBlock>
        <referenceContainer name="main">
            <block 
                class="CB\Helloworld\Block\Computer\ComputetList"
                name="helloworld.computer.list" template="CB_Helloworld::computer/list.phtml" cacheable="false" />
        </referenceContainer>
    </body>
</page>
